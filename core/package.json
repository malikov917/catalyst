{
  "name": "@haiilo/catalyst",
  "version": "6.4.7",
  "description": "Catalyst Design System",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haiilo/catalyst.git"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.js",
  "es2017": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/catalyst/catalyst.esm.js",
  "customElements": "dist/custom-elements/custom-elements.json",
  "files": [
    "dist/",
    "loader/"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "stencil build --docs",
    "build:watch": "stencil build --docs --watch",
    "build:ci": "stencil build --ci",
    "postbuild": "node replace",
    "postbuild:ci": "node replace",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test:watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:style": "stylelint **/*.scss",
    "clean": "rm -rf ./.stencil ./dist ./loader ./www",
    "reset": "rm -rf ./.stencil ./dist ./loader ./www ./node_modules"
  },
  "peerDependencies": {
    "@haiilo/catalyst-icons": ">=0.3.10",
    "rxjs": "^7.5.5"
  },
  "dependencies": {
    "@floating-ui/dom": "1.5.1",
    "@haiilo/catalyst-tokens": "workspace:*",
    "@stencil/core": "4.0.5",
    "autosize": "6.0.1",
    "autosize-input": "1.0.2",
    "flatpickr": "4.6.13",
    "focus-trap": "7.5.2",
    "loglevel": "1.8.1",
    "sanitize.css": "13.0.0",
    "tabbable": "6.2.0",
    "toastify-js": "1.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@haiilo/catalyst-icons": "^1.0.7",
    "@stencil/angular-output-target": "^0.8.0",
    "@stencil/react-output-target": "^0.5.3",
    "@stencil/sass": "^3.0.5",
    "@stencil/vue-output-target": "^0.8.6",
    "@types/autosize": "^4.0.1",
    "@types/jest": "^27.5.2",
    "@types/node": "^18.16.0",
    "@types/toastify-js": "^1.12.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "babel-loader": "^9.1.3",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.7.0",
    "jest-cli": "^27.5.1",
    "prettier": "^3.0.2",
    "puppeteer": "^20.7.3",
    "replace-in-file": "^7.0.1",
    "rxjs": "^7.5.5",
    "stencil-inline-svg": "^1.1.0",
    "stylelint": "^15.10.2",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^10.0.0",
    "typescript": "~4.7.4"
  }
}
